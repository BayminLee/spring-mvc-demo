<?xml version='1.0' encoding='utf-8'?>
<!--映射文件的dtd约束信息-->
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <!-- name代表的是完整类名（包含的类名），table代表的是表名 -->
    <class name="com.tdh.jzgl.entity.TUser" table="T_USER">
        <!-- name代表的是className类中的唯一标识属性，column代表的是tableName表中的主键id -->
        <id name="yhdm" column="YHDM"/>
        <!-- < 其他属性使用property标签映射  name表示className的普通属性 column表示tableName表的普通字段 type 表示字段类型-->
        <property name="dwdm" column="DWDM"/>
        <property name="yhid" column="YHID"/>
        <property name="yhxm" column="YHXM"/>
        <property name="yhkl" column="YHKL"/>
        <property name="yhxb" column="YHXB"/>
        <property name="yhbm" column="YHBM"/>
        <property name="csrq" column="CSRQ"/>
        <property name="djsj" column="DJSJ"/>
        <property name="yhtx" column="YHTX"/>
        <property name="sfjy" column="SFJY"/>
        <property name="pxh" column="PXH"/>
    </class>
</hibernate-mapping>